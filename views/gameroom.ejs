		<!-- Banner -->
	

		<!-- Two -->
		<section id="two" class="spotlight style2 right">
			<div class="gameroom">
				<div class="wrapper">
					<div class="card tck" id="board">
						<div class="panel"></div>
						<div>
							<span id="monitor">
								&cross;
							</span>
						</div>
						<div>
							<ul id="sts">
								<li>Tickets Sold : <%= info.tickets_sold %></li>
									<li>BingoWin Prize : <%= info.BingoWin %> &mu;Ƀ </li>
									<li>BingoLine Prize : <%= info.BingoLine %> &mu;Ƀ </li>
									<li>Ticket Price: 100 &mu;Ƀ </li>
							</ul>
						</div>
					</div>
					
				</div>
				<div class="card" id="chatbox">
					<div class="chat">
						<ul id="chatcontent"></ul>
					</div>
					<div class="controls"><input type="text" id="caption">
						<input type="button" value="send" id="send" disabled></div>
				</div>
			</div>
			<span class="image fit main"><img src="images/pic03.jpg" alt="" /></span>

			<div class="content">
				<header>
					<span>Game Log</span>
					<span> | </span>
					<span> Tickets</span>
					<!--	<p>Nunc commodo accumsan eget id nisi eu col volutpat magna</p>
						-->
